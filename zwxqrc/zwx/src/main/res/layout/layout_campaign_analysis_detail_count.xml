<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    android:layout_marginTop="@dimen/margin_10">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_marginRight="@dimen/margin_10">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/line_color_gray"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:background="#F8F8F8">
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <TextView
                android:id="@+id/tv_coupon"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:gravity="center"
                android:text="优惠券"
                android:textSize="@dimen/font_size_14"
                android:textColor="@color/color_gray_deep"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <TextView
                android:id="@+id/tv_index"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="指标"
                android:textSize="@dimen/font_size_14"
                android:textColor="@color/color_gray_deep"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <TextView
                android:id="@+id/tv_count"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="统计情况"
                android:textSize="@dimen/font_size_14"
                android:textColor="@color/color_gray_deep"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/line_color_gray"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <TextView
                android:id="@+id/tv_coupon_value"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_weight="2"
                android:gravity="center"
                android:text="@string/coupon_value"
                android:textSize="@dimen/font_size_14"
                android:textColor="@color/color_gray_deep"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_send1"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="发放数量"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/color_gray_deep"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_foward1"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="转发数量"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/color_gray_deep"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_receive1"
                    android:layout_width="match_parent"
                    android:layout_height="37dp"
                    android:gravity="center"
                    android:text="回收数量"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/color_gray_deep"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
            </LinearLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_send"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="9999"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/btn_color_blue"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_foward"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="-"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/btn_color_blue"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_receive"
                    android:layout_width="match_parent"
                    android:layout_height="37dp"
                    android:gravity="center"
                    android:text="129"

                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/btn_color_blue"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
            </LinearLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>

        </LinearLayout>
        <!--<View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/line_color_gray"/>-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_weight="2"
                android:orientation="vertical">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_coupon_label"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:gravity="center"
                    android:text="董小姐美味的地锅"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/color_gray_deep"/>
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_send2"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="发放数量"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/color_gray_deep"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_foward2"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="转发数量"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/color_gray_deep"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_receive2"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="回收数量"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/color_gray_deep"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
            </LinearLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_send3"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="-"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/btn_color_blue"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_foward3"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="-"
                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/btn_color_blue"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
                <TextView
                    android:id="@+id/tv_receive3"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="129"

                    android:textSize="@dimen/font_size_14"
                    android:textColor="@color/btn_color_blue"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line_color_gray"/>
            </LinearLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/line_color_gray"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/line_color_gray"/>
    </LinearLayout>



</LinearLayout>