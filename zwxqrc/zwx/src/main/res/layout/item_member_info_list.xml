<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/white"
        android:layout_marginBottom="@dimen/margin_5"
        android:layout_marginTop="@dimen/margin_5">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:gravity="center|left">

            <TextView
                android:id="@+id/tv_tel"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:gravity="center|left"
                android:text=""
                android:textColor="@color/font_color_blue"
                android:textSize="@dimen/font_size_14"
                android:paddingLeft="@dimen/margin_10"
                android:paddingRight="@dimen/margin_10"/>
            <View
                android:id="@+id/line"
                android:layout_width="1dp"
                android:layout_height="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="@dimen/margin_10"
                android:layout_gravity="center"
                android:background="@drawable/ic_line_vertical"/>
            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:gravity="center|left"
                android:text=""
                android:textColor="#303133"
                android:textSize="@dimen/font_size_14"
                android:paddingLeft="@dimen/margin_10"/>
        </LinearLayout>
        <!--  <View
              android:layout_width="match_parent"
              android:layout_height="@dimen/margin_1"
              android:background="@drawable/ic_line_horizontal"/>-->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/margin_10"
            android:paddingTop="@dimen/margin_10">
            <TextView
                android:id="@+id/tv_time_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:layout_marginLeft="@dimen/margin_10"
                android:text="注册时间"
                android:textColor="#909399"
                android:textSize="@dimen/font_size_14"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"/>


            <TextView
                android:id="@+id/tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:layout_marginRight="@dimen/margin_10"
                android:text="2018.11.18 18:22:36"
                android:textColor="@color/color_gray_dark"
                android:textSize="@dimen/font_size_14"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_1"
            android:layout_marginRight="@dimen/margin_10"
            android:layout_marginLeft="@dimen/margin_10"
            android:background="@drawable/ic_line_horizontal"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/margin_10"
            android:paddingTop="@dimen/margin_10">
            <TextView
                android:id="@+id/tv_card_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:layout_marginLeft="@dimen/margin_10"
                android:text="会员卡"
                android:textColor="#909399"
                android:textSize="@dimen/font_size_14"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"/>


            <TextView
                android:id="@+id/tv_memtype_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:layout_marginRight="@dimen/margin_10"
                android:text=""
                android:textColor="@color/color_gray_dark"
                android:textSize="@dimen/font_size_16"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_1"
            android:layout_marginRight="@dimen/margin_10"
            android:layout_marginLeft="@dimen/margin_10"
            android:background="@drawable/ic_line_horizontal"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/margin_10"
            android:paddingTop="@dimen/margin_10">
            <TextView
                android:id="@+id/tv_from_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:layout_marginLeft="@dimen/margin_10"
                android:text="会员来源"
                android:textColor="#909399"
                android:textSize="@dimen/font_size_14"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"/>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:gravity="center">


                <TextView
                    android:id="@+id/tv_from"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center|left"
                    android:layout_marginRight="@dimen/margin_10"
                    android:text="fsdfsdfsdfsdfsd"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textColor="@color/color_gray_dark"
                    android:textSize="@dimen/font_size_16"/>
                <TextView
                    android:id="@+id/tv_campaign_from"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginRight="@dimen/margin_10"
                    android:text=""
                    android:background="@drawable/shape_btn_blue_normal"
                    android:textColor="@color/color_gray_deep"
                    android:textSize="@dimen/font_size_12"
                    android:padding="5dp"/>
            </LinearLayout>



        </RelativeLayout>


    </LinearLayout>




</LinearLayout>